{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "css.validate": false,
  "stylelint.enable": true,
  "vetur.validation.style": false,
  "cSpell.words": [
    "tailwindcss"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "tailwindCSS.includeLanguages": {
    "vue": "html",
    "vue-html": "html",
  },
}
